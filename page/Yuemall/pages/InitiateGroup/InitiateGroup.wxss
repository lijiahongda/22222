/* page/Mall/pages/InitiateGroup/InitiateGroup.wxss */

page {
  background-color: #ececec;
}

.goodInfo {
  width: 88%;
  margin-left: 4%;
  margin-right: 4%;
  padding-left: 2%;
  padding-right: 2%;
  padding-top: 22rpx;
  padding-bottom: 22rpx;
  border-radius: 16rpx;
  background-color: #fff;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.goodInfo image {
  width: 240rpx;
  height: 240rpx;
  margin-right: 30rpx;
  background-color: #ffac49;
}

.goodInfo .info {
  display: flex;
  flex-direction: column;
}

.goodInfo .info .title text {
  color: #303030;
  font-size: 30rpx;
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-bottom: 25rpx;
}

.goodInfo .info .groupInfo {
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 30rpx;
  margin-bottom: 23rpx;
}

.goodInfo .info .groupInfo .title {
  color: #303030;
}

.goodInfo .info .groupInfo .price {
  color: #ed5d2f;
}

.goodInfo .info .OriginalPrice {
  color: #999;
  font-size: 26rpx;
}

.NumberGroups {
  width: 82%;
  margin-left: 4%;
  margin-right: 4%;
  padding-left: 5%;
  padding-right: 5%;
  padding-top: 30rpx;
  padding-bottom: 26rpx;
  border-radius: 16rpx;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 30rpx;
  margin-bottom: 20rpx;
  position: relative;
}

.NumberGroups .title {
  color: #303030;
  font-size: 30rpx;
  margin-bottom: 58rpx;
}

.NumberGroups .title text {
  color: #ff6d5b;
}

.NumberGroups .HeadPortrait {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap
}

.NumberGroups .HeadPortrait .image {
  position: relative;
  margin-right: 40rpx;
  margin-left: 33rpx;
  margin-top: 20rpx
}

.NumberGroups .HeadPortrait .image .head {
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
}

.NumberGroups .HeadPortrait .image .RegimentalCommander {
  position: absolute;
  bottom: 0rpx;
  width: 123rpx;
  height: 30rpx;
  left: 5.5rpx;
}

/* 倒计时 */

.CountDown {
  display: flex;
  flex-direction: row;
  color: #fff;
  font-size: 34rpx;
  justify-content: center;
  align-items: center;
  margin-top: 71rpx;
  margin-bottom: 66rpx;
}

.CountDown .list {
  height: 48rpx;
  width: 48rpx;
  background-color: #303030;
  border-radius: 4rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.CountDown .colon {
  margin-left: 12rpx;
  margin-right: 12rpx;
  color: #303030;
}

.CountDown .tips {
  color: #303030;
  font-size: 26rpx;
  margin-left: 24rpx;
}

.InviteFriends {
  width: 100%;
  height: 74rpx;
  border-radius: 16rpx;
  background: linear-gradient(90deg, rgba(255, 110, 68, 1), rgba(255, 112, 92, 1), rgba(255, 78, 73, 1));
  color: #fff;
  font-size: 32rpx;
  text-align: center;
  line-height: 74rpx;
  margin-bottom: 66rpx;
}

.AnotherRound {
  width: 92%;
  height: 74rpx;
  border-radius: 16rpx;
  background: linear-gradient(90deg, rgba(255, 80, 74, 1) 0%, rgba(255, 112, 91, 1) 100%);
  color: #fff;
  font-size: 32rpx;
  text-align: center;
  line-height: 74rpx;
  margin-bottom: 66rpx;
  margin-top: 66rpx;
}

.orderDetail {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding-bottom: 26rpx;
  border-bottom: 1px solid #ececec;
  color: #999;
  font-size: 26rpx;
  width: 100%;
}

.orderDetail .info {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 74%;
}

.orderDetail .info text {
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
}

.orderDetail .info image {
  width: 14rpx;
  height: 22rpx;
  margin-left: 14rpx;
}

.OrderRules {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding-top: 26rpx;
  color: #999;
  font-size: 26rpx;
  width: 100%;
}

/* 邀请好友 */

.Invitation {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 52rpx;
  background-color: #fff;
  padding-top: 30rpx;
  padding-bottom: 34rpx;
}

.Invitation .titlemore {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 37rpx;
  width: 62%;
  margin-left: 30%;
}

.Invitation .titlemore .more {
  display: flex;
  flex-direction: row;
  align-items: center;
  color: #999;
  font-size: 20rpx;
}

.Invitation .titlemore .more image {
  width: 14rpx;
  height: 22rpx;
  margin-left: 18rpx;
}

.Invitation .title {
  font-size: 36rpx;
  font-family: STYuanti-SC-Bold;
  font-weight: bold;
  color: rgba(247, 206, 224, 1);
  background: linear-gradient(157deg, rgba(255, 112, 91, 1) 0%, rgba(255, 79, 74, 1) 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}

.Invitation .shopList {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  width: 92%;
}

.Invitation .shopList .list {
  display: flex;
  flex-direction: column;
  /* align-items: center; */
  width: 43%;
  padding-left: 2.5%;
  padding-right: 2.5%;
  padding-top: 12rpx;
  padding-bottom: 38rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 0px 20px 0px rgba(0, 0, 0, 0.1);
  border-radius: 10px;
}

.Invitation .shopList .list image {
  height: 318rpx;
  width: 100%;
  background-color: #ffac49;
}

.Invitation .shopList .list .goodtitle {
  font-size: 24rpx;
  color: #333;
  font-weight: bold;
  margin-top: 20rpx;
}

.Invitation .shopList .list .goodtitle text {
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 12px;
  line-height: 18px;
  height: 35px;
}

.Invitation .shopList .list .goodInfo {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100%;
}

.Invitation .shopList .list .goodInfo .price {
  font-size: 30rpx;
  color: #fd6974;
}

.Invitation .shopList .list .goodInfo .price text {
  font-size: 30rpx;
}

.SpellNum {
  font-size: 26rpx;
  color: #999;
  width: 88%;
  margin-left: 4%;
  margin-right: 4%;
  padding-left: 2%;
  padding-right: 2%;
}

.Invitation .shopList .list .goodInfo .HeadPortrait {
  position: relative;
  width: 22%;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
}

.Invitation .shopList .list .goodInfo .HeadPortrait image {
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
}

.Invitation .shopList .list .goodInfo .HeadPortrait image.one {
  background-color: #ff4c48;
  left: 0rpx;
  z-index: 10rpx;
}

.Invitation .shopList .list .goodInfo .HeadPortrait image.two {
  background-color: #ff7666;
  left: 20rpx;
}

button[class="con_listbox"]::after {
  border: 0;
}

button {
  width: 100%;
  height: 100%;
  background-color: #fff;
  border: 0;
  margin: 0;
  padding: 0;
  border-color: #fff;
  box-shadow: 0;
  line-height: 1;
}

button::after {
  border: none;
}

.Success {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  margin-top: 66rpx;
  margin-bottom: 66rpx;
}

.Success .lookorder {
  width: 266rpx;
  height: 66rpx;
  border: 1px solid #303030;
  border-radius: 16rpx;
  color: #303030;
  font-size: 32rpx;
  line-height: 66rpx;
  text-align: center;
}

.Success .MoreBenefits {
  width: 268rpx;
  height: 68rpx;
  border-radius: 16rpx;
  color: #fff;
  font-size: 32rpx;
  line-height: 68rpx;
  text-align: center;
  background: linear-gradient(90deg, rgba(255, 80, 74, 1) 0%, rgba(255, 112, 91, 1) 100%);
}

.SuccessImage {
  width: 91rpx;
  height: 84rpx;
  position: absolute;
  top: 0;
  right: 0rpx;
}

.JoinOthers {
  height: 186rpx;
  width: 100%;
  background: rgba(255, 243, 238, 1);
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin-bottom: 28rpx;
  border-radius: 14rpx;
}

.JoinOthers .JoinOthersImage {
  width: 442rpx;
  height: 24rpx;
  margin-top: 28rpx;
  margin-bottom: 28rpx;
}

.GroupList {
  padding-bottom: 20rpx;
  width: 92%;
  padding-left: 4%;
  padding-right: 4%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.GroupList .SenderInfo {
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 24rpx;
  color: #000;
  width: 39%;
}

.GroupList .SenderInfo view {
  width: 42%;
}

.GroupList .SenderInfo text {
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
}

.GroupList .SenderInfo image {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  background-color: #ff7666;
  margin-right: 20rpx;
}

.GroupList .GroupInfo {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.GroupList .GroupInfo .info {
  display: flex;
  flex-direction: column;
}

.GroupList .GroupInfo .info .fewPeople {
  color: #0a0204;
  font-size: 20rpx;
  margin-bottom: 15rpx;
}

.GroupList .GroupInfo .info .fewPeople text {
  color: #ff4e49;
}

.GroupList .GroupInfo .info .time {
  color: #999;
  font-size: 20rpx;
}

.GroupList .GroupInfo .GoGroup {
  width: 110rpx;
  height: 50rpx;
  background: linear-gradient(90deg, rgba(255, 111, 91, 1) 0%, rgba(255, 78, 73, 1) 100%);
  border-radius: 6rpx;
  font-size: 24rpx;
  color: #fff;
  text-align: center;
  line-height: 50rpx;
  margin-left: 22rpx;
}
