  flex-direction: column;
  align-items: center;
}
/* 授权按钮 */
.authorizationStatus{
  position: fixed;
  z-index: 10000;
  width: 100%;
  height: 100%;
  background-color: transparent;
  top: 0rpx;
  margin-top: 0rpx
}
.topImage{
  height: 489rpx;
  width: 100%;
  display: block
}
.contentImage{
  width: 100%;
  display: block;
}
.content{
  display: flex;
  flex-direction: column;
  position: absolute;
  width: 90%;
  margin-top: 470rpx;
}
.item{
  display: flex;
  flex-direction: column;
  justify-items: center;
  width: 100%;
  align-items: center
}
.item .titleImage{
  width: 626rpx;
  height: 178rpx;
  margin-left: 83rpx;
}
.item .list{
  margin-top: -49rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.item .list .goodImage{
  width: 500rpx;
  height: 590rpx;
  margin-bottom: 40rpx;
}
.Source{
  display: flex;
  flex-direction: column;
  width: 100%;
  justify-content: center;
  align-items: center;
  color:#FF8585;
  font-size: 26rpx;
  line-height: 45rpx;
}