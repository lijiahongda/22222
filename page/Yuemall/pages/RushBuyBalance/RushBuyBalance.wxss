  color:rgba(20,20,20,1);
}
.typeAlert>.list{
  width: 100%;
  height: 120rpx;
  padding:0 25rpx 0 25rpx;
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;
  line-height: 100rpx;
}
.typeAlert>.list>.listname{
