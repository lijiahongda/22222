/* page/payment/component/code/index.wxss */
.codeAlert{
  width: 600rpx;
  height: 391rpx;
  background:rgba(255,255,255,1);
  border-radius:16rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  z-index: 10;
}
.codeAlert>.text{
  width: 100%;
  font-size:32rpx;
  text-align: center;
  line-height: 32rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(20,20,20,1);
  margin-top: 60rpx;
  margin-bottom: 60rpx;
}
.codeAlert>.close{
  width: 33rpx;
  height: 33rpx;
  text-align: center;
  line-height: 33rpx;
  font-size: 33rpx;
  position: absolute;
  right: 30rpx;
  top: 30rpx;
  color: #ddd;
}
.codeAlert>.inputCode{
  width: 518rpx;
  height: 80rpx;
  border-bottom: 2rpx solid rgba(221,221,221,1);
  margin: 0 auto;
  box-sizing: border-box;
  margin-bottom:20rpx;
}
.codeAlert>.inputCode>input{
  text-align: center;
  width: 100%;
  height: 100%;
  line-height: 80rpx;
  font-size:26rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(20,20,20,1);
}
.codeAlert>.sText{
  text-align: center;
  line-height: 24rpx;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
  margin-bottom: 92rpx;
  margin-top: 60rpx;
}
.codeAlert>.alertBt{
  width:518rpx;
  height:86rpx;
  background:rgba(202,37,25,1);
  text-align: center;
  line-height: 86rpx;
  border-radius:16rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  margin: 0 auto;
  margin-top: 20rpx;
}
.alert{
  width:100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 2000;
}
.typeAlert{
  width:100%;
  height: 500rpx;
  border-radius:18rpx 18rpx 0rpx 0rpx;
  background:rgba(255,255,255,1);
  position: absolute;
  bottom: -500rpx;
  left:0;
  transition: all ease 0.5s;
}
.typeAlert>.title{
