}
.typeAlert>.list>.listname text{
  font-size:26rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(102,102,102,1);
}
.typeAlert>.list>.listname>.img{
  width: 60rpx;
  height: 60rpx;
  margin-top: 20rpx;
  margin-right: 20rpx;
}
.typeAlert>.list>.listname>.img>image{
