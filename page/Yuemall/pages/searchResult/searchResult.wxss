page{
  background: rgba(246,246,246,1);
}
.header{
  width: 100%;
  background: #fff;
  border-radius:0px 0px 10rpx 10rpx;
}
.search{
  height: 88rpx;
  width: 100%;
  padding: 0 24rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.search .search-con{
  width: 90%;
  height: 60rpx;
  line-height: 60rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  color: #999;
  display: flex;
  align-items: center;
  background:rgba(246,246,246,1);
  border-radius:30rpx;
}
.search .search-con image{
  width: 26rpx;
  height: 26rpx;
  margin-right: 20rpx;
}
.search .search-type{
  width: 26rpx;
  height: 26rpx;
}
.sort{
  display: flex;
  align-items: center;
  justify-content: space-around;
  height: 100rpx;
}
.sort .list{
  text-align: center;
  padding: 26rpx 10rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #333;
  line-height: 28rpx;
  border-bottom: 4rpx solid #fff;
  display: flex;
  align-items: center;
}
.sort .list.active{
  border-bottom: 4rpx solid #FA937E;
}
.sort .price-icon image{
  width: 16rpx;
  height: 8rpx;
  margin-left: 12rpx;
  display: block;
}
.sort .price-icon .price-up{
  margin-bottom: 8rpx;
}
.sort .list.screen image{
  margin-left: 8rpx;
  width: 28rpx;
  height: 28rpx;
}
/* 分类 */
.category{
  width:100%;
  padding-bottom: 26rpx;
}
.category .nav {
  height: 54rpx;
  width: 100%;
  /* overflow: hidden; */
  /* white-space: nowrap; */
  margin: 24rpx 0;
  margin-bottom: 0;
  padding-left: 30rpx;
  box-sizing: border-box;
  white-space: nowrap;
}
::-webkit-scrollbar{

    width: 0;
    
    height: 0;
    
    color: transparent;

}
.disync{
  display: flex;
  height: 28rpx;
}
.disync > text{
  margin: 10rpx 0;
  padding: 0 10rpx;
  height: 28rpx;
  background:url('https://yuetao-1300766538.cos.ap-beijing.myqcloud.com/yuetao/image/2020-02-23/13/yuelvhuiHknJ4n6IIQ1582435354.png') no-repeat;
  background-size: 100% 100%;
  display: flex;
  align-items: center;
  justify-content: center;
font-size:22rpx;
font-family:PingFang;
font-weight:500;
color:rgba(255,255,255,1);
margin-right: 10rpx;
}
.category .nav-item {
  display: inline-block;
  text-align: center;
  margin-right: 42rpx;
  font-size: 24rpx;
  color: #333;
  font-weight: 400;
  width:140rpx;
  height:44rpx;
  line-height: 44rpx;
  background:rgba(246,246,246,1);
  border-radius:22px;
}
.category .activeChild{
  color: #fff;
  background: rgba(250,147,126,1);
}

/* --------------- */
/* 横 */
.content {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.content .item {
  display: flex;
  flex-direction: row;
  background-color: #fff;
  width: 702rpx;
  border-radius: 10rpx;
  padding: 20rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
  position: relative;
}

.content .item .image {
  width: 260rpx;
  height: 260rpx;
  margin-right: 20rpx;
  position: relative;
}
.content .item .image .Ranking{
  position: absolute;
  right: 0;
  top: 10rpx;
  background: #B94D46;
  font-size:24rpx;
  color: #fff;
  height: 36rpx;
  line-height: 36rpx;
  padding: 0 22rpx;
  border-radius:18rpx 0px 0px 18rpx;
}

.content .item .image image {
  width: 260rpx;
  height: 260rpx;
  background-color: #ececec;
}

.content .item .info {
  display: flex;
  flex-direction: column;
}

.content .item .info .Surplus {
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #999;
  margin-top: 26rpx;
  margin-bottom: 10rpx;
}

.content .item .info .pricebutton {
  display: flex;
  flex-direction: row;
  color: #999;
  font-size: 24rpx;
  justify-content: space-between;
  margin-top: 20rpx;
}

.content .item .info .pricebutton .price .Company {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #B94D46;
}

.content .item .info .pricebutton .price text {
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #B94D46;
}

.content .item .info .pricebutton .price .OriginalPrice {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  text-decoration: line-through;
  color: #666666;
  margin-left: 20rpx;
}

/* 暂无商品 */
.NoCommodityData{
  width: 100%;
  padding-top: 150rpx;
  text-align: center;
  font-size: 24rpx;
  color: #B3B3B3
}
.NoCommodityData image{
  width: 172rpx;
  height: 166rpx;
  margin-bottom: 44rpx;
}





/* ========= */

/* 内容区 */
/* 方 */
.ContentArea {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  width: 94%;
  padding-left: 3%;
  justify-content: space-between;
  margin-top: 27rpx;
}

.ContentArea .item {
  width: 49%;
  background-color: #fff;
  border-radius: 12rpx;
  margin-bottom: 20rpx;
  padding: 16rpx;
  box-sizing: border-box;
}
.ContentArea .item .shopImageLeft{
  position: relative;
}
.ContentArea .item .source{
  position: absolute;
  right: 0;
  top: 10rpx;
  background: #B94D46;
  height: 36rpx;
  line-height: 36rpx;
  font-size: 24rpx;
  color: #fff;
  padding: 0 20rpx;
  border-radius: 18rpx 0px 0px 18rpx;
}

.ContentArea .item image {
  width: 100%;
  height: 329rpx;
  background-color: #f4f4f4;
}

.ContentArea .item .itemInfo .title {
  color: #333;
  font-size: 28rpx;
  margin-bottom: 28rpx;
  margin-right: 24rpx;
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
}
.titleH{
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
  vertical-align: middle;
}
.bigLtags{
  width: 100%;
  height: 45rpx;
  display: flex;
  margin-top: 10rpx;
}
.bigLtags>.viptag{
  width:174rpx;
  height:44rpx;
  background:rgba(27,23,22,1);
  border-radius:10rpx;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(218,219,220,1);
  line-height:44rpx;
  display: flex;
}
.bigLtags>.viptag image,.bigLtags>.nomtag image{
  width: 40rpx;
  height: 40rpx;
}
.bigLtags>.nomtag{
  width:188rpx;
  height:44rpx;
  background:rgba(255,244,245,1);
  border:2rpx solid rgba(220,0,0,1);
  opacity:0.8;
  border-radius:8rpx;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(220,0,0,1);
  line-height:44rpx;
  text-align: center;
  margin-left: 15rpx;
}
.ContentArea .item .itemInfo .hot {
  font-size: 22rpx;
  line-height: 22rpx;
  color: #999;
  margin-bottom: 20rpx;
}

.ContentArea .item .itemInfo .price {
  font-size: 22rpx;
  color: #B94D46;
  display: flex;
  flex-direction: row;
  line-height: 36rpx;
  flex-wrap: wrap;
}
.ContentArea .item .itemInfo .price .old-price{
  color: #666;
  font-size: 24rpx;
  text-decoration-line: line-through;
}



/* 筛选弹框 */
.black-grd{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0,0,0,0.2);
  z-index: 1
}
.screen-shell,.screen-con{
  position: fixed;
  top: 0;
  right: 0;
  width: 538rpx;
  height: 100%;
  background: #fff;
  border-radius: 10rpx 0 0 10rpx;
  z-index: 9;
  padding: 30rpx 24rpx 0 12rpx;
  box-sizing: border-box;
}
.screen-con{
  overflow-y: auto;
  height: 90%;
  border-radius:0;
}
.screen-type .title{
  display: flex;
  align-content: center;
  justify-content: space-between;
  font-size: 30rpx;
  color: #333;
  line-height: 30rpx;
  margin-bottom: 32rpx;
}
.screen-type .title image{
  width: 24rpx;
  height: 14rpx;
  margin-top: 8rpx;
}
.screen-type .screen-list{
  display: flex;
  align-items: center;
  flex-wrap: wrap
}
.screen-type .screen-list view,.screen-type .price-input input{
  /* width: 138rpx; */
  /* padding: 0 10rpx; */
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 24rpx;
  background: rgba(235,235,235,1);
  font-size: 24rpx;
  color: #999;
  margin-right: 40rpx;
  margin-bottom: 32rpx;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.screen-type .screen-list view.chose{
  background: rgba(185,77,70,1);
  color: #fff;
}
.screen-type .price-input{
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 42rpx;
}
.screen-type .price-input input{
  width: 212rpx;
  margin: 0;
}
.screen-type .price-input .line{
  width: 40rpx;
  height:3rpx;
  background: rgba(153,153,153,1);
}
.screen-btn{
  position: fixed;
  bottom: 0;
  right: 0;
  display: flex;
  align-items: center;
  width: 538rpx;
  height: 10%;
  padding: 20rpx 0;
  box-sizing: border-box;
}
.screen-btn view{
  width:212rpx;
  height:74rpx;
  line-height: 74rpx;
  text-align: center;
  border-radius:10rpx;
  font-size: 30rpx;
}
.screen-btn view.reset{
  border:2rpx solid rgba(235,235,235,1);
  background: #fff;
  color: #333;
  margin-right: 32rpx;
  margin-left: 40rpx;
}
.screen-btn view.sure{
  background:rgba(185,77,70,1);
  border: none;
  color: #fff;
}
.wph{
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
  margin-right: 6rpx;
  display: inline-block;
}