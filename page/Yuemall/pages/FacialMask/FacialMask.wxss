/* page/Yuemall/pages/FacialMask/FacialMask.wxss */
page{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center
}
.topImages{
  width: 100%;
  height: 480rpx;
  display: block
}
.contentImage{
  width: 100%;
  display: block
}
.content{
  position: absolute;
  display: flex;
  flex-direction: column;
  /* width: 325rpx; */
  padding-left: 20rpx;
  padding-right: 20rpx;
  height: 137rpx;
  border-radius: 10rpx;
  background-color: #FF573A;
  top: 300rpx;
  /* left: 30%; */
  color:#fff;
  font-size: 24rpx;
  justify-content: center;
  align-items: center
}
.CountDown{
  display: flex;
  flex-direction: row;
  margin-top: 18rpx;
  color:#D11500;
  font-size: 34rpx;
  justify-content: center;
  align-items: center
}
.CountDown .list{
  height: 60rpx;
  width: 60rpx;
  background-color: #F9EDDF;
  border-radius: 4rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center
}
.CountDown .colon{
  margin-left: 12rpx;
  margin-right: 12rpx
}