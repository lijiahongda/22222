@import "/page/LoadingPopup/LoadingPopup.wxss";
page {
  background-color: #fff;
}
.dang_classification{
  margin-bottom: 30rpx
}
.dang_classification .hr {
  width: 100%;
  height: 1px;
  background: rgba(221, 221, 221, 1);
}
.dangSearch{
  width:700rpx;
  height:60rpx;
  margin-left:25rpx;
  margin-right:25rpx;
  display:flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom:-5rpx;
}
.search{
  width:614rpx;
  background:#F6F6F6;
  font-size:24rpx;
  height:60rpx;
  border-radius:30rpx;
  padding-left:40rpx;
  box-sizing: border-box;
  display:flex;
  flex-direction: row;
  align-items: center;
}
.searchImg{
  width:26rpx;
  height:25rpx;
  padding-right:16rpx;
  margin-left:-15rpx;
}
.dang_classification .dang_classification_content {
  margin: 0 24rpx;
}

.dang_classification_content_model {
  margin-top: 40rpx;
}

.dang_content_model {
  display: flex;
}

.content_model_image, .content_model_image image {
  width: 44rpx;
  height: 44rpx;
}

.content_model_text {
  margin-left: 20rpx;
  height: 31rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(20, 20, 20, 1);
}

.content_model_textarea {
  margin-top: 30rpx;
}

.content_model_textarea_text {
  height: 80rpx;
  padding-bottom: 26rpx;
  text-align: center;
  display: inline-block;
  width: 24%;
  border: 1px solid rgba(221, 221, 221, 1);
  margin: 0 0 -4px -1px;
  white-space: nowrap;
text-overflow: ellipsis;
overflow: hidden;

}
