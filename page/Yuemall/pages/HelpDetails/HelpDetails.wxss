/* page/Mall/pages/HelpDetails/HelpDetails.wxss */
.HasEnded{
  width: 132rpx;
  height: 120rpx;
  position: absolute;
  right: 33rpx;
  top: 55rpx;
}
.Helpbg {
  background-image: url(https://image.yuelvhui.com/pubfile/2019/04/12/line_1555059992.png);
  height: 616rpx;
  background-size: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding-top: 40rpx;
  padding-bottom: 20rpx;
  background-repeat: no-repeat;
  background-color: #ff4c42;
  position: relative;
}

.Helpbg .HelpDetails {
  display: flex;
  flex-direction: column;
  padding-left: 3%;
  padding-right: 3%;
  width: 86%;
  border-radius: 16rpx;
  background-color: #fff;
  padding-bottom: 61rpx;
}

.goodInfo {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-bottom: 20rpx;
  padding-top: 30rpx;
}

.goodInfo image {
  width: 240rpx;
  height: 240rpx;
  margin-right: 30rpx;
  background-color: #ffac49;
}

.goodInfo .info {
  display: flex;
  flex-direction: column;
}

.goodInfo .info .title text {
  color: #303030;
  font-size: 30rpx;
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-bottom: 19rpx;
}

.goodInfo .info .groupInfo {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  font-size: 22rpx;
  margin-bottom: 23rpx;
}

.goodInfo .info .groupInfo .title {
  color: #999;
  font-size: 26rpx;
}

.goodInfo .info .groupInfo .price {
  color: #ff7666;
}

/* 倒计时 */

.CountDown {
