button::after {
  border: none;
}

.BargainList {
  background-color: #ff4c42;
  width: 92%;
  padding-left: 4%;
  padding-right: 4%;
  padding-bottom: 20rpx;
  display: flex;
  flex-direction: column;
}

.BargainList .PriceList {
  background-color: #fff;
  padding-top: 29rpx;
  padding-bottom: 38rpx;
  border-radius: 16rpx;
  width: 94%;
  padding-left: 3%;
  padding-right: 3%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.BargainList .PriceList .title {
  font-size: 30rpx;
  color: #303030;
  font-weight: 400;
}

.GroupList {
  border-bottom: 1px solid #ececec;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  width: 92%;
  padding-left: 4%;
  padding-right: 4%;
  background-color: #fff;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.GroupList .SenderInfo {
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 24rpx;
  color: #000;
  width: 53%;
}
.GroupList .SenderInfo view{
  width: 40%
}
.GroupList .SenderInfo text {
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
}

.GroupList .SenderInfo image {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  background-color: #ff7666;
  margin-right: 20rpx;
}

.GroupList .GroupInfo {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.GroupList .GroupInfo .GoGroup {
  font-size: 30rpx;
  color: #ff6f5b;
}

.lookMore{
  display: flex;
  flex-direction: row;
  width: 100%;
  align-items: center;
  justify-content: center;
  color: #666;
  font-size: 24rpx;
}
.lookMore image{
  margin-left: 14rpx;
  height: 12rpx;
  width: 20rpx
}
.Invitation {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 52rpx;
  background-color: #fff;
  padding-top: 30rpx;
  padding-bottom: 34rpx;
}
.Invitation .titlemore{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 37rpx;
  width: 62%;
  margin-left: 30%;
}
.Invitation .titlemore .more{
  display: flex;
  flex-direction: row;
  align-items: center;
  color:#999;
  font-size: 20rpx;
}
.Invitation .titlemore .more image{
  width: 14rpx;
  height: 22rpx;
  margin-left: 18rpx;
}
.Invitation .title {
  font-size: 36rpx;
  font-family: STYuanti-SC-Bold;
  font-weight: bold;
  color: rgba(247, 206, 224, 1);
  background: linear-gradient(157deg, rgba(255, 112, 91, 1) 0%, rgba(255, 79, 74, 1) 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}

.Invitation .shopList {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  width: 92%;
}

.Invitation .shopList .list {
  display: flex;
  flex-direction: column;
  /* align-items: center; */
  width: 43%;
  padding-left: 2.5%;
  padding-right: 2.5%;
  padding-top: 12rpx;
  padding-bottom: 38rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 0px 20px 0px rgba(0, 0, 0, 0.1);
  border-radius: 10px;
}

.Invitation .shopList .list image {
  height: 318rpx;
  width: 100%;
  background-color: #ffac49;
}

.Invitation .shopList .list .goodtitle {
  font-size: 24rpx;
  color: #333;
  font-weight: bold;
  margin-top: 20rpx;
}

.Invitation .shopList .list .goodtitle text {
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 12px;
  line-height: 18px;
  height: 35px;
}

.Invitation .shopList .list .goodInfo {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100%;
}

.Invitation .shopList .list .goodInfo .price {
  font-size: 30rpx;
  color: #fd6974;
}

.Invitation .shopList .list .goodInfo .price text {
  font-size: 30rpx;
}

.Invitation .shopList .list .goodInfo .SpellNum {
  font-size: 18rpx;
  color: #999;
  margin-top: 32rpx;
}

.Invitation .shopList .list .goodInfo .HeadPortrait {
  position: relative;
  width:22%;
  display: flex;
  flex-direction: row;
  justify-content: flex-end
}

.Invitation .shopList .list .goodInfo .HeadPortrait image {
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
}

.Invitation .shopList .list .goodInfo .HeadPortrait image.one {
  background-color: #ff4c48;
  left: 0rpx;
  z-index: 10rpx;
}

.Invitation .shopList .list .goodInfo .HeadPortrait image.two {
  background-color: #ff7666;
  left: 20rpx
}


/* 分享层 */

.share-layer {
  background: rgba(0, 0, 0, 0.6);
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 10001;
}

.share-layer-mask {
  width: 100%;
  height: 100%;
}

.share-layer-block {
  position: absolute;
  bottom: 105rpx;
  left: 4%;
  width: 92%;
  /* border-radius: 28rpx; */
  border-top-left-radius: 28rpx;
  border-top-right-radius: 28rpx;
  height: 150rpx;
  background: #fff;
  text-align: center;
  z-index: 2;
  display: flex;
  flex-direction: row;
}
.share-layer-cancel{
  position: absolute;
  bottom: 30rpx;
  left: 4%;
  width: 92%;
  border-bottom-left-radius: 28rpx;
  border-bottom-right-radius: 28rpx;
  height: 75rpx;
  background: #fff;
  z-index: 2;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center
}
.share-layer-cancel view{
  width: 92%;
  border-top: 1px solid #DEDEDE;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 75rpx;
}
.share-layer-block-img {
  width: 100%;
  height: 100%;
  /* float: left; */
}

.share-layer-block image {
  margin-top: 21rpx;
  width: 130rpx;
  height: 110rpx;
}

.share-layer-block-img button {
  width: 100%;
  height: 100%;
  background: #fff;
  border: 0;
  margin: 0;
  padding: 0;
  border-color: #fff;
  box-shadow: 0;
  border-radius: 28rpx;
}

.share-layer-block-img button::after {
  border: none;
}
