  margin-bottom: 17rpx;
}

.CountDown .list {
  height: 48rpx;
  width: 48rpx;
  background-color: #303030;
  border-radius: 4rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.CountDown .colon {
  margin-left: 12rpx;
  margin-right: 12rpx;
  color: #303030;
}

.CountDown .tips {
  color: #303030;
  font-size: 26rpx;
