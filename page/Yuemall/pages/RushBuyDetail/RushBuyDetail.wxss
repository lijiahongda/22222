/* 秒杀价格 */

.seckill {
  width: 100%;
  height: 110rpx;
  background: #ca2519;
  display: flex;
  justify-content: space-between;
}

.seckill .price {
  width: 532rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding-left: 24rpx;
  box-sizing: border-box;
}

.seckill .price > view:nth-child(1) {
  /* display: flex; *//* justify-content: baseline; */
  font-size: 24rpx;
  color: #fff;
  vertical-align: bottom;
}

.seckill .price > view:nth-child(1) > text:nth-child(2) {
  font-size: 42rpx;
  padding-left: 4rpx;
}

.seckill .price > view:nth-child(2) {
  display: flex;
  justify-content: baseline;
  font-size: 24rpx;
  color: #f4b2b2;
}

.seckill .price > view:nth-child(2) > text:nth-child(1) {
  padding-right: 40rpx;
  text-decoration: line-through;
}

.seckill .time {
  width: 218rpx;
  height: 110rpx;
  background-image: url(https://yuetao-1300766538.cos.ap-beijing.myqcloud.com/yuetao/image/2020-01-17/21/yuelvhuiBO8RkPKLuR1579266749.png);
  background-size: 100% 100%;
}

.seckill .time > image {
  width: 138rpx;
  height: 26rpx;
  display: block;
  margin: 14rpx 0 14rpx 50rpx;
}

.seckill .time .showTime {
  display: flex;
  justify-content: center;
  align-items: center;
}

.seckill .time .showTime > view:nth-child(2n-1) {
  background: #fbffd2;
  border-radius: 8rpx;
  width: 40rpx;
  height: 40rpx;
  color: #30302e;
  font-size: 24rpx;
  text-align: center;
  line-height: 40rpx;
}

.seckill .time .showTime > view:nth-child(2n) {
  color: #fbffd2;
  font-size: 24rpx;
  padding: 0 4rpx;
}

.seckillWill .price > view:nth-child(1) > text:last-child {
  color: #f4b2b2;
  text-decoration: line-through;
  padding-left: 14rpx;
}

.seckillWill .price > view:nth-child(2) {
  color: #fbffd2;
  align-items: center;
  padding-bottom: 10rpx;
}

.seckillWill .price > view:nth-child(2) > text:nth-child(1) {
  padding-right: 40rpx;
  background: #fbffd2;
  text-align: center;
  padding: 4rpx 8rpx;
  border-radius: 20rpx;
  color: #30302e;
  text-decoration: none;
}

.seckillWill .price > view:nth-child(2) > text:nth-child(2) {
  padding-left: 10rpx;
}

.seckillWill .time .showTime {
  color: #fbffd2;
  font-size: 24rpx;
}

