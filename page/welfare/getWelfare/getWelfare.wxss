/* page/welfare/getWelfare/getWelfare.wxss */
.page{
  position: absolute;
  width: 100%;
  height: 100%;
  background:rgba(0,0,0,.6);
}
.page-body{
  width: 586rpx;
  height: 838rpx;
  position: absolute;
  top: 164rpx;
  left: 82rpx;
  background: url(https://image.yuelvhui.com/pubfile/2019/10/12/line_1570887738_47281.png) no-repeat center;
  background-size: contain;
  text-align: center;
  font-size:28rpx;
font-family:PingFang SC;
font-weight:400;
color:rgba(255,255,255,1);
}
.page-body text{
  display: block;
}
text.title{
  font-size:40rpx;
font-weight:500;
color:rgba(227,103,84,1);
margin: 59rpx 0 50rpx;
}
text.money{
  font-size:75rpx;
  height: 75rpx;
  line-height: 75rpx;
font-weight:500;
color:rgba(235,78,79,1);
} 
text.money text{
  font-size:46rpx;
  display: inline-block
}
text.welfare-type{
  font-size:32rpx;
font-weight:400;
color:rgba(213,181,123,1);
margin: 30rpx 0 79rpx;
}
text.info{
  font-size:28rpx;
font-weight:400;
color:rgba(255,255,255,1);
margin-bottom: 50rpx;
}
.page-body image.photo{
  width:99rpx;
height:100rpx;
border: 1px solid #eee;
border-radius:50%;
margin-top: 64rpx;
display: block;
margin: 64rpx auto 30rpx;
}
navigator{
  position: absolute;
  width: 68rpx;
  height: 68rpx;
  right: 95rpx;
  top: 56rpx;
  background: url(https://image.yuelvhui.com/pubfile/2019/10/12/line_1570879607_81817.png) no-repeat center;
  background-size: contain;
}

input{
  color:rgba(253,250,240,1);
  font-size: 32rpx;
  border-bottom: 1rpx solid rgba(255,166,152,1);
  width: 447rpx;
  height: 50rpx;
  display: block;
  padding-bottom: 19rpx;
  margin: 0 auto;
  text-align: left;
}
view input.phone{
  margin-bottom: 20rpx;
}
button{
  font-size:32rpx;
font-family:PingFang SC;
font-weight:500;
color:rgba(142,39,0,1);
  width:480rpx;
height:88rpx;
background:rgba(249,214,119,1);
box-shadow:4rpx 9rpx 20rpx 0rpx rgba(239,60,38,0.29);
border-radius:44rpx;
margin-top: 40rpx;
}