page{
  background: #EFEEF3;
  padding-top: 20rpx;
}
.contentInfo{
  background-color: #fff;
  padding-left: 4%;
  padding-right: 4%;
  width: 92%;
  display: flex;
  flex-direction: column;
}
.contentInfo .item{
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #ececec;
}
.contentInfo .item image.tits{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50rpx;
}
.contentInfo .item .title{
  color:#333;
  font-size: 28rpx;
}
.contentInfo .item image{
  width: 12rpx;
  height: 22rpx;
  margin-left: 20rpx;
}
.contentInfo .item .info{
  display: flex;
  flex-direction: row;
  align-items: center;
  color:#cacaca;
}