/* page/oneself/pages/myWalletContent/myWalletContent.wxss */

page {
  background-color: #f8f8f8;
}

.Commission {
  background-image: url(https://image.yuelvhui.com/pubfile/2019/06/12/line_1560323564.png);
  background-repeat: no-repeat;
  background-size: 100%;
  height: 345rpx;
  display: flex;
  flex-direction: column;
}

.Commission .HistoricalCommission {
  display: flex;
  flex-direction: column;
  height: 228rpx;
  width: 100%;
  justify-content: center;
  align-items: center;
}

.Commission .HistoricalCommission image {
  width:80rpx;
  height:80rpx;
  border-radius: 50%;
  border:1px solid #fff;
  margin-top: 80rpx;
  margin-bottom:24rpx;
}

.Commission .HistoricalCommission .AccumulatedAmount {
  font-size: 30rpx;
  color: #fff;
}

.FunctionalCommission {
  height: 142rpx;
  width: 92%;
  margin-left: 4%;
  border-radius: 10rpx;
  display: flex;
  flex-direction: row;
  align-items: flex-end;
  background-color: #fff;
  position: absolute;
  top: 280rpx
}

.FunctionalCommission .Division {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-right: 1px solid #ececec;
  width: 49%;
  height: 128rpx;
  color: #666;
}
.FunctionalCommission .Division  image{
  width:12rpx;
  height:22rpx;
}
.FunctionalCommission .Division .title {
  font-size: 24rpx;
  margin-bottom: 22rpx;
}

.tab-h {
  height: 74rpx;
  width: 100%;
  box-sizing: border-box;
  /* line-height: 80rpx; *//* background-color: #F8F8F8; */
  font-size: 16px;
  white-space: nowrap;
  z-index: 99;
  display: flex;
  align-items: center;
  /* box-shadow: 0 4rpx 8px #ddd; *//* background-color: #ff6636; */
}

.tab-item {
  margin: 0 3%;
  /* background-color: #ff6636; */
  width: 30%;
  font-size: 30rpx;
  color: #000;
  display: inline-block;
  text-align: center;
  position: relative;
  z-index: 10000;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  /* line-height: 100rpx */
}

.tab-item.active {
  color: #ff6032;
}

.tab-item.active:after {
  content: "";
  display: block;
  width: 100%;
  /* background-repeat: no-repeat;
  height: 45rpx;
  background-image: url(http://image.yuelvhui.com/pubfile/2018/10/23/line_1540262428.png);
  background-color: #ff6636; */
  position: absolute;
  top: 53rpx;
  left: 35%;
}

.tab-con image {
  width: 100%;
  height: 100%;
}

.tab-con {
  /* padding: 25rpx 0; */
  min-height: 100%;
  position: relative;
}

.tab-con.gwc {
  padding-bottom: 125rpx;
}

.scoll-h {
  background-color: #fff;
  width: 95%;
  margin-left: 2.5%;
  margin-right: 2.5%;
  border-radius: 10rpx;
}
.CommonFunction{
  background-color: #fff;
  height: 91rpx;
  display: flex;
  flex-direction: row;
  border-bottom: 1px solid #f7f7f7;
  width: 92%;
  margin-left: 4%;
  align-items: center;
  margin-top: 93rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx
  
}
.CommonFunction image{
  margin-left:30rpx;
}
.CommonFunction image{
  width: 7rpx;
  height: 34rpx;
}
.CommonFunction .borderLeft{
  color: #333;
  font-size: 34rpx;
  margin-left: 3.6%;
}

.list {
  height: 166rpx;
  width: 87%;
  margin-left: 4%;
  padding-left: 2.5%;
  padding-right: 2.5%;
  background-color: #fff;
  border-bottom: 1px solid #ececec;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.list .PersonnelInfo {
  display: flex;
  flex-direction: row;
  align-items: flex-start;
  width:60%;
}

.list .PersonnelInfo image {
  width: 80rpx;
  height: 80rpx;
  border: 1px solid #ffb536;
  border-radius: 50%;
  margin-right: 22rpx;
  margin-left: 6rpx;
  background-color: #e4393c;
}

.list .PersonnelInfo .name {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-bottom: 20rpx;
}

.list .PersonnelInfo .time {
  margin-bottom: 14rpx;
}

.list .PersonnelInfo .name .series {
  width: 100rpx;
  height: 30rpx;
  border-radius: 6rpx;
  background-color: #ff6032;
  color: #fff;
  line-height: 30rpx;
  text-align: center;
  font-size: 20rpx;
  /* margin-left: 20rpx; */
}
.list .PersonnelInfo .name .nameText{
  /* width:40% */
}
.list .PersonnelInfo .name .nameText text {
  font-size: 28rpx;
  color: #333;
  overflow:hidden;
  white-space:nowrap !important;
  text-overflow:ellipsis;
  display:block;
}
.list .StatusSum{
  display: flex;
  flex-direction: column;
  width:40%;
  align-items: flex-end;
}
.list .StatusSum image{
  width:12rpx;
  height:22rpx;
  margin-bottom:20rpx
}
.list .status{
  width:68rpx;
  height:30rpx;
  text-align: center;
  line-height: 30rpx;
  border-radius: 6rpx;
  color:#fff;
  background-color: #ccc;
  font-size: 20rpx;
}
.list .Amount{
  color:#ccc;
  font-size:26rpx;
  /* margin-top:52rpx */
}
.list .Amount text{
  font-size: 30rpx
}

