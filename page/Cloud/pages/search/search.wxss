page {
  background-color: #fff;
}

.vessel {
  display: flex;
  flex-direction: column;
}

.searchVessel {
  background-color: #fff;
  padding-bottom: 12rpx;
}
.search-cont{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 10rpx 24rpx;
}

.search {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 88%;
  background-color: rgb(248, 248, 248);
  height: 60rpx;
  border-radius: 30rpx;
}

.search input {
  margin-left: 10rpx;
  width: 100%;
  font-size: 24rpx;
}

.placeholder {
  color: rgb(153, 153, 153);
  font-size: 26rpx;
}

.search image {
  width: 28rpx;
  height: 28rpx;
  margin-left: 16rpx;
}

.RecentSearch {
  margin-left: 3%;
  margin-right: 3%;
  font-size: 26rpx;
  color: #666
}

.RecentSearch .title {
  font-size: 26rpx;
  font-weight: 400;
  color: #666;
  width: 100%;
  margin-top: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.RecentSearch .title image {
  width: 20rpx;
  height: 21rpx;
}

.RecentSearch .RecentContent {
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.RecentSearch .search-title{
  font-size: 26rpx;
  color: #666;
  margin-top: 50rpx;
}

.RecentSearch .RecentContent view {
  height: 66rpx;
  line-height: 66rpx;
  background-color: rgba(246,246,246,1);
  border-radius: 32rpx;
  text-align: center;
  padding: 0 26rpx;
  margin-right: 24rpx;
  margin-top:24rpx;
  font-size: 26rpx;
  color: #212121;
}

.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}

.commodity_attr_box {
  width: 100%;
  position: fixed;
  z-index: 1990;
  background: #fff;
}

.close {
  display: flex;
  justify-content: flex-end;
}

.close icon {
  margin-right: 3%;
}

.commodity {
  width: 95%;
  margin-top: 20rpx;
}

.commodity .list {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  border-bottom: 1px solid rgba(244,244,244,1);
  padding-bottom: 24rpx;
  padding-top: 24rpx;
  font-size: 26rpx;
  line-height: 44rpx;
}
.commodity .list .title{
  margin-left: 20rpx;
  /* margin-right: 120rpx; */
  width: 100%;
}
.commodity .list .content{
  padding: 0 24rpx;
  background-color: rgba(246,246,246,1);
  border-radius: 22rpx;
  color:#333;
  font-size: 24rpx;
  height: 44rpx;
  line-height: 44rpx;
  margin-left: 12rpx;
  margin-top: 10rpx;
}
.commodity view text {
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
}