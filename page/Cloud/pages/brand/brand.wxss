page{
  background: #ECECEC;
}
.header{
  position: relative;
  width: 100%;
  height: 330rpx;
}
.header .top-bg{
  width:100%;
  height: 200rpx;
  background: #FA937E;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 0;
}
.header-cont{
  position: absolute;
  left: 20rpx;
}
/* 轮播 */
.header .swiper-box{
  width: 710rpx;
  margin-top: 22rpx;
  height: 300rpx;
}
.header .swiper-box .image{
  width: 100%;
  height: 300rpx;
  border-radius: 10rpx;
}

.brand_list{
  border-radius: 10rpx 10rpx 0 0;
  background: #fff;
  width: 702rpx;
  display: flex;
  align-items: center;
  margin: 0 auto;
  padding: 10rpx;
  box-sizing: border-box;
  flex-wrap: wrap;
  justify-content: space-between;
  margin-top: 10rpx;
}
.list{
  width: 33%;
  border-right: 1rpx solid rgba(236,236,236,1);
  border-bottom: 1rpx solid rgba(236,236,236,1);
  height: 175rpx;
  text-align: center;
  display: flex;
  align-items: center;
}
.list:nth-child(3n){
  border-right: none;
}
.list image{
  max-width: 80%;
  margin: 0 auto;
  max-height: 80%;
}
