/* page/yueMember//pages/UniversityList/UniversityList.wxss */

page {
  background-color: #fff;
  border-top: 1px solid #eee;
}

.list {
  margin-left: 2.6%;
  margin-right: 2.6%;
  width: 94.8%;
  display: flex;
  flex-direction: row;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  border-bottom: 1px solid #eee;
}

.list image {
  width: 200rpx;
  height: 150rpx;
  border-radius: 10rpx;
  background-color: #eee;
  margin-right: 29rpx;
}

.list .info {
  display: flex;
  flex-direction: column;
  width: 66%;
}

.list .info .title text {
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(17, 17, 17, 1);
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
}

.list .info .time {
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(17, 17, 17, 1);
  margin-top: 38rpx
}
