page {
  height: 100%;
}

/* 授权按钮 */

.authorizationStatus {
  position: fixed;
  z-index: 10000;
  width: 100%;
  height: 100%;
  background-color: transparent;
  top: 0rpx;
  margin-top: 0rpx;
}

image {
  width: 100%;
  height: 100%;
  /* background-color: #2A2A2A; */
}

view.image {
  height: 100%;
  width: 100%;
  position: relative;
}

.HotelReservation {
  width: 100%;
  height: 78rpx;
  background-color: #f8c258;
  position: fixed;
  bottom: 0rpx;
  font-size: 40rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 78rpx;
  text-align: center;
}

.currencyButton {
  display: flex;
  flex-direction: row;
  color: #f9c25b;
  font-size: 33rpx;
  align-items: center;
  justify-content: center;
  width: 237rpx;
  height: 70rpx;
  background-color: #855608;
  border-radius: 25rpx;
  position: absolute;
  top: 39%;
  left: 36%;
}

.currencyButton image {
  height: 24rpx;
  width: 14rpx;
  margin-left: 16rpx;
}

.saveMoney {
  display: flex;
  flex-direction: row;
  font-size: 40rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 78rpx;
  background-color: #ff664c;
  position: fixed;
  bottom: 0rpx;
}

.saveMoney image {
  height: 24rpx;
  width: 14rpx;
  margin-left: 16rpx;
}

.Coupon {
  display: flex;
  flex-direction: row;
  color: #fff;
  font-size: 33rpx;
  align-items: center;
  justify-content: center;
  width: 237rpx;
  height: 70rpx;
  background-color: #c9543a;
  border-radius: 25rpx;
  position: absolute;
  top: 53%;
  left: 35%;
}

.Coupon image {
  height: 24rpx;
  width: 14rpx;
  margin-left: 16rpx;
}

.AlreadyOpened {
  width: 100%;
  height: 109rpx;
  background-color: #0A0A0A;
  display: flex;
  flex-direction: row;
  font-size: 36rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  align-items: center;
  justify-content: center;
  position: fixed;
  bottom: 0rpx;
}

.YearCard {
  width: 237rpx;
  height: 70rpx;
  position: absolute;
  top: 83%;
  left: 34%;
}
.button{
  width: 100%;
  border-radius: 16rpx;
  background: #fff;
  color: #333333;
  font-size: 32rpx;
  text-align: center;
  line-height: 80rpx;
  height: 80rpx;
  font-family:PingFang SC;
  font-weight:600;
  position: fixed;
  bottom: 20rpx;
}