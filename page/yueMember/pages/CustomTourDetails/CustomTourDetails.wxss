}
.foot{
  display: flex;
  flex-direction: row;
  position: absolute;
  bottom:-rpx;
  width:100%
}
.foot .Consultation{
  height:100rpx;
  width:33%;
  background-color: #fff;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  color:#333;
  font-size: 32rpx;
}
.foot .Consultation image{
