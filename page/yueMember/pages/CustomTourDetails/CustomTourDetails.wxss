/* pages/yueMember/CustomTourDetails/CustomTourDetails.wxss */

page {
  height: 100%;
}

.tab-h {
  height: 80rpx;
  width: 100%;
  box-sizing: border-box;
  line-height: 80rpx;
  background: #fff;
  font-size: 16px;
  white-space: nowrap;
  z-index: 99;
  display: flex;
  flex-direction: row;
}

.tab-h image {
  width: 40rpx;
  height: 28rpx;
}

.tab-item {
  margin: 0 17%;
  /* width: 20%; */
  display: flex;
  text-align: center;
  align-content: center;
  align-items: center;
  position: relative;
}

.tab-item.active {
  color: #f4aa11;
  /*position: relative;*/
}

.tab-item.active:after {
  content: "";
  display: block;
  height: 6rpx;
  width: 100%;
  background: #f4aa11;
  position: absolute;
  align-content: center;
  bottom: 0;
}

.tab-con image {
  width: 100%;
  height: 100%;
}

.tab-con {
  padding: 25rpx 0;
  min-height: 100%;
  position: relative;
}

.tab-con.gwc {
  padding-bottom: 125rpx;
}

.item {
  display: flex;
  flex-direction: column;
  width: 100%;
  margin: auto;
  margin-bottom: 41rpx;
  background-color: #fff;
  padding-bottom: 30rpx;
}

.item .image {
  height: 336rpx;
  background-size: cover;
  background-repeat: no-repeat;
  display: flex;
  flex-direction: row;
  justify-content: end;
  align-items: flex-end;
}

.item .image .note {
  background-color: rgba(0, 0, 0, 0.34);
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 96%;
  height: 44rpx;
  padding-left: 2%;
  padding-right: 2%;
  color: #fff;
  font-size: 22rpx;
  align-items: center;
}

.item .content {
  width: 95%;
  margin: auto;
  display: flex;
  flex-direction: column;
}

.item .content .lineTitle {
  color: #303030;
  font-size: 28rpx;
  margin-top: 18rpx;
}

.item .content .introduce {
  width: 83%;
  margin-top: 18rpx;
}

.item .content .introduce text {
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #999;
  font-size: 26rpx;
}

.item .content .money {
  color: #f4aa11;
  margin-top: 22rpx;
}

.item .content .unit {
  font-size: 24rpx;
}

.item .content .num {
  font-size: 38rpx;
}

.item .content .qi {
  font-size: 26rpx;
}

.item .content .save {
  color: #666;
  font-size: 24rpx;
  margin-left: 3%;
}

.item .content .labels {
  display: flex;
  flex-direction: row;
  margin-top: 30rpx;
}
.circle{
  width:16rpx;
  height:16rpx;
  border:1px solid#f4aa11;
  border-radius: 50%;
  position: relative;
  left:-6.7%;
  bottom:-10rpx
}
.item .content .labels .lists {
  border: 1px solid #08c2ea;
  width:20%;
  /* padding-left: 2%; */
  /* padding-right: 2%; */
  height: 34rpx;
  color: #08c2ea;
  line-height: 34rpx;
  text-align: center;
  font-size: 22rpx;
  margin-right: 2%;
}

.CustomerRecommends {
  margin-top: 20rpx;
  background-color: #fff;
  width: 90%;
  padding-left: 5%;
  padding-right: 5%;
}

.CustomerRecommends .title {
  height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  color: #303030;
  font-size: 28rpx;
  border-bottom: 1px solid #ececec;
}

.CustomerRecommends .title image {
  width: 32rpx;
  height: 34rpx;
  margin-right: 2%;
}

.CustomerRecommends .CustomerContent {
  display: flex;
  flex-direction: column;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.CustomerRecommends .CustomerContent .ContentLabel {
  width: 23%;
  color: #999;
}

.CustomerRecommends .CustomerContent .contentItem {
  display: flex;
  flex-direction: row;
  width: 100%;
  margin-bottom: 27rpx;
  font-size: 26rpx;
}

#Id0 {
  background-color: #fff;
  width: 90%;
  padding-left: 5%;
  padding-right: 5%;
  padding-top: 30rpx;
