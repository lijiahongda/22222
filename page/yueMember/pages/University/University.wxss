/* page/yueMember//pages/University/University.wxss */

page {
  background: #f6f6f6;
  padding: 3% 4%;
}
swiper{
  width: 92%;
  height: 250rpx;
}
swiper image{
  width: 100%;
  height: 250rpx;
}
.top {
  width: 92%;
  height: 250rpx;
  position: relative;
}

.universitys {
  width: 200rpx;
  height: 150rpx;
  position: absolute;
  top: 55rpx;
  left: 105rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.universitys text {
  width: 215rpx;
  height: 51rpx;
  font-size: 52rpx;
  color: rgba(255, 251, 251, 1);
  -webkit-background-clip: text;
  margin-bottom: 35rpx;

}
.makeMoney {
  width: 88%;
  padding-top: 35rpx;
  padding-left: 4%;
  background-color: #fff;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  border-radius: 10px;
  margin-top: 20rpx;
}
.makeMoney .item {
  width: 47%;
  display: flex;
  color: rgba(48, 48, 48, 1);
  margin-bottom: 55rpx;
}

.makeMoney .item image {
  width: 71rpx;
  height: 71rpx;
  border-radius: 50%;
  margin-bottom: 14rpx;
}
.describe{
  display: flex;
  flex-direction: column;
  margin-left: 10%;
}
.makeMoney .item .txt{
  color: #333;
  font-size: 28rpx;
}
.makeMoney .item .tips {
  font-size: 20rpx;
  font-weight: 400;
  color: #999;
  margin-top: 6rpx;
}

.University {
  width: 92%;
  padding-top: 35rpx;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  border-radius: 10px;
  margin-top: 20rpx;
}

.UniversityContent {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  width: 96%;
  margin-top: 5rpx;
  padding-left: 4%;
}

.University .title {
  margin-left: 30rpx;
  display: flex;
  width: 100%;
  justify-content: flex-start;
  flex-direction: row;
  align-items: center;
  font-size: 32rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  margin-bottom: 24rpx;
}

.University .title image {
  width: 7rpx;
  height: 35rpx;
  margin-right: 18rpx;
}

.UniversityContent .item {
  width: 47%;
  display: flex;
  align-items: center;
  margin-bottom: 34rpx;
}

.UniversityContent .item image {
  width: 76rpx;
  height: 76rpx;
  border-radius: 50%;
  margin-bottom: 14rpx;
}

.UniversityContent .item .tips {
  font-size: 24rpx;
  color: #747474;
  margin-top: 6rpx;
}