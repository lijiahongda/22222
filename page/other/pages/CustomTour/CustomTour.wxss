.tab-h {
  height: 100rpx;
  width: 100%;
  box-sizing: border-box;
  line-height: 100rpx;
  background: #fff;
  font-size: 16px;
  white-space: nowrap;
  z-index: 99;
}

.tab-item {
  width: 100%;
  color: #f4aa11;
  display: inline-block;
  text-align: center;
  position: relative;
  font-size: 32rpx;
  transform: skewX(0deg);
  background-image: url(https://zhiliao-pic.oss-cn-beijing.aliyuncs.com/334429154255523525.png);
  background-size: 100%;
  background-repeat: no-repeat;
}

.tab-item.active {
  color: #fff;
  /* background-color: rgb(77,211,153) */
}

.tab-item.active:after {
  display: block;
  height: 6rpx;
  width: 100%;
  background: #1ebe62;
  position: absolute;
  bottom: 0;
  left: 5rpx;
}

.tab-con image {
  width: 100%;
  height: 100%;
}

.tab-con {
  /* padding: 25rpx 0; */
