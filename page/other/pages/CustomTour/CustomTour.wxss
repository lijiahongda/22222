checkbox .wx-checkbox-input {
  width: 30rpx; /* 背景的宽 */
  height: 30rpx; /* 背景的高 */
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border: none;
  background: #f4aa11;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  border-radius: 50%; /* 圆角 */
  width: 40rpx; /* 选中后对勾大小，不要超过背景的尺寸 */
  height: 40rpx; /* 选中后对勾大小，不要超过背景的尺寸 */
  line-height: 40rpx;
  text-align: center;
  font-size: 24rpx; /* 对勾大小 30rpx */
  color: #fff; /* 对勾颜色 白色 */
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}

.placeholder {
  color: rgb(192, 192, 192);
}

.case {
  background-color: #fff;
  width: 100%;
  margin-top: 32rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.case .caeetitle {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  height: 126rpx;
}

.case .caeetitle .wire {
  color: #ccc;
}

.case .caeetitle .tit {
  color: #f4aa11;
  font-size: 30rpx;
  margin-left: 3%;
  margin-right: 3%;
}

.case .item {
  display: flex;
  flex-direction: column;
  width: 90%;
  margin: auto;
  margin-bottom: 41rpx;
}

.case .item .image {
  height: 336rpx;
  background-size: cover;
  background-repeat: no-repeat;
  display: flex;
  flex-direction: row;
  justify-content: end;
  align-items: flex-end;
}

.case .item .image .note {
  background-color: rgba(0, 0, 0, 0.34);
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 96%;
  height: 44rpx;
  padding-left: 2%;
  padding-right: 2%;
  color: #fff;
  font-size: 22rpx;
  align-items: center;
}

.case .item .lineTitle {
  color: #303030;
  font-size: 28rpx;
  margin-top:18rpx
}

.case .item .introduce{
  width:83%;
  margin-top:18rpx
}
.case .item .introduce text{
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  color:#999;
  font-size: 26rpx
}
 .case .item .money {
  color: #f4aa11;
  margin-top: 22rpx;
}
.case .item .unit {
  font-size: 24rpx;
}
.case .item .num {
  font-size: 38rpx;
}
.case .item .qi {
  font-size: 26rpx;
}
.case .item .save{
  color:#666;
  font-size: 24rpx;
  margin-left:3%
}