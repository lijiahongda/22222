/* page/other/pages/utourletLuckyDraw/utourletLuckyDraw.wxss */
.wrap{
  width: 100%;
  height: auto;
  overflow: hidden;
}
.wrap .banner{
  width: 100%;
  height: 350rpx;
  margin-bottom: 60rpx;
}
.wrap .banner image{
  width: 100%;
  height: 100%;
}
.wrap .utourletLuckyDraw{
  width: 100%;
  overflow: hidden;
  height: 190rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 120rpx;
}

.wrap .utourletLuckyDraw image{
  width: 330rpx;
  height: 190rpx;
  border-radius: 10rpx;
}

.wrap .process{
  width: 690rpx;
  height: 70rpx;
  border-radius: 30rpx;
  text-align: center;
  line-height: 70rpx;
  display: block;
  margin: 0 auto;
  color: #000;
  font-size: 30rpx;
  background-color: #FCCE16;
}