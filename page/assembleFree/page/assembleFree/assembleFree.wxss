/* 底部 */
.footer {
  display: flex;
  align-items: center;
  justify-content: space-around;
  font-size: 22rpx;
  color: rgb(155, 155, 155);
  position: fixed;
  background-color: #fff;
  bottom: 0;
  width: 100%;
  text-align: center;
  z-index: 1990;
  height: 98rpx;
  border-top:2rpx solid rgba(0,0,0,0.2);
  box-sizing: border-box;
}
.footer > .left{
  width: 50%;
  height: 100%;
  color: #999999;
  font-size: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding-top: 8rpx;
  box-sizing: border-box;
  position: relative;
}
.footer > .left > image{
  width: 36rpx;
  height: 36rpx;
  margin-bottom: 8rpx;
}
.footer > .left .shopNum{
  position: absolute;
  top: 4rpx;
  right: 0rpx;
  width: 34rpx;
  height: 34rpx;
  color: #fff;
  background: #F22300;
  text-align: center;
  line-height: 34rpx;
  font-size: 24rpx;
  border-radius: 50%;
}
.footer > .right{
  width: 460rpx;
  height: 80rpx;
  border-radius: 40rpx;
  background: #2D2D2D;
  color: #fff;
  display: flex;
  position: absolute;
  bottom: 8rpx;
  right: 24rpx;
}
.footer > .right > .red{
  background: #CA2519;
}
.footer > .right > view{
  width: 50%;
  height: 100%;
  text-align: center;
  line-height: 80rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.footer > .right > view.double{
  line-height: 30rpx;
  font-size: 22rpx;
}
.footer > .right > view.double > view:nth-child(2){
  font-size: 28rpx;
}
.footer > .right > .join{
  width: 100%;
  color: #FCE893;
  font-size: 30rpx;
}
.footer > .right > .allRed{
  width: 100%;
  background: #CA2519;
  color: #fff;
  font-size: 30rpx;
}
.tab-h {
  height: 99rpx;
  width: 100%;
  box-sizing: border-box;
  line-height: 80rpx;
  background: #fff;
  white-space: nowrap;
  z-index: 99;
  border-bottom-left-radius: 30rpx;
  border-bottom-right-radius: 30rpx;
  display: flex;
  justify-content: space-around;
}
.tab-item {
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(112, 112, 112, 1);
  margin:0 3%;
  width: 20%;
  display: inline-block;
  text-align: center;
  position: relative;
}

.tab-item.active {
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #141414;
}

.tab-item.active:after {
  content: "";
  display: block;
  height: 5rpx;
  width: 100%;
  background: #CA2519;
  position: absolute;
  bottom: 0;
}

.tab-con image {
  width: 100%;
  height: 100%;
}

.tab-con {
  min-height: 100%;
  position: relative;
}

.tab-con.gwc {
  padding-bottom: 125rpx;
}

.scoll-h {
  height: 100%;
  position: relative;
}
