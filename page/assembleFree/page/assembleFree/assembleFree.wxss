.goodInfo {
  background-color: #fff;
  width: 100%;
  margin: 0 auto;
  margin-bottom: 20rpx;
  padding: 20rpx 24rpx;
  box-sizing: border-box;
  border-radius: 18rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.goodInfoH {
  padding-bottom: 0;
  margin-bottom: 0;
}
.goodInfo .goodsImg {
  width: 260rpx;
  height: 260rpx;
  margin-right: 38rpx;
  background: url(https://yuetao-1300766538.cos.ap-beijing.myqcloud.com/yuetao/image/2019-12-19/14/yuelvhuiU8Luv9ia7f1576736515.png);
  background-size: 243rpx 243rpx;
}
.goodInfo .info {
  width: 100%;
  display: flex;
  flex-direction: column;
}
.goodInfo .info .title text {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #141414;
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-bottom: 20rpx;
}

.goodInfo .info .introduction text {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #666666;
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-bottom: 20rpx;
  width: 302rpx!important;
}
.goodInfo .info .groupInfo {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 10rpx;
}
.groupInfoCon{
  display: flex;
  align-items: center;
  margin-bottom: 10rpx;
}
.goodInfo .info .groupInfo .price {
  font-size: 38rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #C82519;
}
.goodInfo .info .groupInfo .price text {
  font-size: 24rpx;
}

.goodInfo .info .OriginalPrice {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  text-decoration: line-through;
  color: #BBBBBB;
  margin-left: 10rpx;
  margin-top: 10rpx;
}

.ClusteringNum {
