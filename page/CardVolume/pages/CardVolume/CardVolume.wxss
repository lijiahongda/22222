page {
  background-color: #F7F5F6;
  height: 100%;
  width: 100%;
  padding-bottom: 90rpx;
  /*  */
}

/* 最新优惠券 */

.item {
  width: 100%;
  /* height: 180rpx; */
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-bottom: 20rpx;
  background-color: #FFFEFF;
  margin-top: 19rpx;
  border-radius: 18rpx;
  padding-top: 200rpx;
  position: relative;
  overflow: hidden;
}

.itemImage {
  position: absolute;
  top: 0;
  left: 0;
  width: 220rpx;
}

.PreferentialAmount {
  font-size: 48rpx;
  font-family: Adobe Heiti Std R;
  font-weight: normal;
  color: rgba(255, 255, 255, 1);
}

.PreferentialAmount .text {
  font-size: 24rpx;
}
.imageInfo .showShare{
  position: absolute;
  top: 0;
  left: 0;
  width: 118rpx;
  height: 86rpx;
  display: block;
}

.desc {
  font-size: 24rpx;
  font-family: Adobe Heiti Std R;
  font-weight: normal;
  color: rgba(255, 255, 255, 1);
}

.imageInfo {
  display: flex;
  flex-direction: column;
  position: absolute;
  top: 0rpx;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
}

.item image {
  width: 220rpx;
  height: 200rpx;
  display: block;
}

.itemInfo {
  position: absolute;
  left: 0;
  top: 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 200rpx;
  width: 482rpx;
  margin-left: 220rpx;
  background: #FFFFFF;
  box-shadow:0px 5rpx 15rpx 0px rgba(208,208,208,0.2);
}

.itemInfo .title {
  display: flex;
  flex-direction: row;
  font-size: 34rpx;
  color: #333;
  align-items: center;
  margin-top: 24rpx;
  width: 429rpx;
}
.instruction{
  padding: 18rpx 44rpx 18rpx 20rpx;
  width: 100%;
  box-sizing: border-box;
  /* word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis; */
  position: relative;
  line-height: 46rpx;
}
.instruction.close{
  height: 80rpx;
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
}
.instruction.open{
  height: auto;
}
.instruction > image{
  position: absolute;
  right: 20rpx;
  top: 36rpx;
  width: 20rpx;
  height: 12rpx;
}
.instruction::after{
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 18rpx;
  background: #fff;
}
.instruction image{
  width: 20rpx;
  height: 12rpx;
  display: inline-block;
  margin-left: 18rpx;
}

.bar {
  max-width: 57%;
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(26, 26, 26, 1);
}

.bar text {
  overflow: hidden;
  white-space: nowrap !important;
  text-overflow: ellipsis;
  display: block;
}

.tag {
  height: 40rpx;
  background: #F6AA07;
  border-radius: 20rpx 20rpx 20rpx 0;
  padding: 0 10rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(253, 237, 237, 1);
  text-align: center;
  line-height: 40rpx;
  margin-right: 10rpx;
}

.itemInfo .line{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 429rpx;
}

.time {
  margin-top: 17rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #666666;
}

.ImmediateUse {
  width: 130rpx;
  height: 48rpx;
  border: 2rpx solid #EC455B;
  border-radius: 24rpx;
  color: #EC455B;
  font-size: 24rpx;
  line-height: 48rpx;
  text-align: center;
  margin-top: 19rpx;
  box-sizing: border-box;
}

.count {
  position: absolute;
  top: 0rpx;
  right: 0rpx;
  height: 42rpx;
  background: #F67D07;
  line-height: 42rpx;
  text-align: center;
  font-size: 26rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  border-radius:0px 18rpx 0px 18rpx;
  padding: 0 8rpx;
}
.hasCount{
  width: auto;
  height: 44rpx;
  border: 2rpx solid #F6F6F6;
  padding: 0 10rpx;
  box-sizing: border-box;
  position: absolute;
  left: -2rpx;
  bottom: 0;
  text-align: center;
  line-height: 44rpx;
  font-size: 24rpx;
  color: #fff;
  border-radius: 22rpx 22rpx 22rpx 0;
}

.AlreadyUsed {
  border: 1px solid #dadada;
  color: #dadada;
}

